{
    "name":"CryptoZoon Tools",
    "description":"crypto-zoon-tools",
    "version":"0.1.0",
    "manifest_version":3,
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
    	"service_worker": "background.js"
    },
    "action":{
        "default_popup":"popup.html",
        "default_icon":{
            "16":"./img/icon.png",
            "32":"./img/icon.png",
            "48":"./img/icon.png",
            "128":"./img/icon.png"
        }
    },
    "icons":{
        "16":"./img/icon.png",
        "32":"./img/icon.png",
        "48":"./img/icon.png",
        "128":"./img/icon.png"
    },
    "permissions":["activeTab", "tabs", "storage", "scripting", "notifications", "alarms"]
}